{
   "compilerOptions": {
      "outDir": "dist",
      "sourceMap": true,
      "target": "es2018",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node"],
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
   },
   "include": ["src/**/*.ts", "bot.ts"],
   "exclude": ["node_modules"]
}
